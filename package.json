{
  "name": "grace-shopper",
  "version": "1.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "precommit": "# lint-staged # un-comment to enable",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\""
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@opuscapita/react-searchbar": "^1.0.6",
    "axios": "^0.19.0",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "express-session": "^1.15.1",
    "helmet": "^3.14.0",
    "history": "^4.6.3",
    "morgan": "^1.9.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.3.1",
    "react-search-bar": "^2.2.0",
    "react-stripe-elements": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.82.5",
    "sequelize": "^4.41.0",
    "stripe": "^6.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^3.5.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-config-fullstack": "^5.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "npm-merge-driver": "^2.3.5",
    "prettier": "1.11.1",
    "react-test-renderer": "^16.5.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "supertest": "^3.3.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.3.12"
  }
}
